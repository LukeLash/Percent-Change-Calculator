{
  "name": "Percent Change Calculator",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Excel is for bums",
  "permissions": ["activeTab"],

  "chrome_url_overrides": {

  },

  "icons": {
     "16": "percentLogo16.png",
     "48": "percentLogo48.png",
     "128": "percentLogo128.png"
 },

 "browser_action": {
   "default_icon": "percentLogo48.png",
   "default_popup": "popup.html",
   "default_title": "Percent Change Calculator"
 },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["popup.css"],
      "js": ["popup.js"]
    }
  ],

"background": {
  "scripts": ["popup.js"]
}

}
